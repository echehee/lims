            <title>Budget Application</title>
            <short-title>Budget Application</short-title>
            <keywords></keywords>
        </portlet-info>
        <security-role-ref>
            <role-name>administrator</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>guest</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>power-user</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>user</role-name>
        </security-role-ref>
    </portlet>
    <portlet>
        <portlet-name>project</portlet-name>
        <display-name>Project</display-name>
        <portlet-class>com.startarget.portlet.ProjectPortlet</portlet-class>
        <init-param>
            <name>view-template</name>
            <value>/html/project/view.jsp</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>

        <supported-locale>en</supported-locale>
        <supported-locale>zh_CN</supported-locale>
        <resource-bundle>content.project.Language</resource-bundle>

        <portlet-info>
            <title>Project</title>
            <short-title>Project</short-title>
            <keywords></keywords>
        </portlet-info>
        <security-role-ref>
            <role-name>administrator</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>guest</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>power-user</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>user</role-name>
        </security-role-ref>
    </portlet>
    <portlet>
